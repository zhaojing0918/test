<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <!-- 关于接口的映射文件 -->
<mapper namespace="edu.zj.dao01.StudentDao"> 
<!-- id必须与方法名相同  parameterType参数类型-->
<insert id="insertStudent" parameterType="edu.zj.pojo.Student">
	INSERT INTO student(NAME) VALUES(#{name});
</insert>
</mapper>